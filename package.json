{
  "name": "core",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "create_db": "node dist/db/create_db.js",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc -p tsconfig.json --outDir dist",
    "build-queue": "tsc -p jobQueue/tsconfig.json --outDir dist/jobQueue",
    "debug": "node --nolazy --inspect-brk=9229 dist/index.js",
    "prestart": "npm run build",
    "test": "nyc mocha --exit",
    "codecv": "./node_modules/.bin/codecov",
    "start": "node dist/index.js",
    "start_frontend": "cd frontend & npm run startp",
    "watch": "concurrently \"nodemon\" \"npm run start_frontend\"",
    "queue": "node dist/jobQueue/jobQueue/runner.js",
    "checkdep": "madge --circular dist"
  },
  "author": "Michal Stepien",
  "license": "ISC",
  "dependencies": {
    "@babel/parser": "^7.5.5",
    "bull": "^3.11.0",
    "cls-hooked": "^4.2.2",
    "commander": "^2.20.0",
    "dotenv": "^8.0.0",
    "ensure-login": "^0.1.4",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.2",
    "jsonwebtoken": "^8.5.1",
    "orientjs": "^3.0.5",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "puppeteer": "^1.20.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "se-scraper": "^1.5.6",
    "vm2": "^3.8.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/bull": "^3.10.3",
    "@types/chai": "^4.2.0",
    "@types/cls-hooked": "^4.3.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-jwt": "0.0.42",
    "@types/express-session": "^1.15.13",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.2",
    "@types/passport": "^1.0.0",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-local": "^1.0.33",
    "@types/puppeteer": "^1.20.2",
    "@types/redis": "^2.8.14",
    "@types/swagger-ui-express": "^3.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codecov": "^3.5.0",
    "concurrently": "^4.1.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "swagger-ui-express": "^4.0.7",
    "ts-morph": "^3.1.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "nyc": {
    "exclude-after-remap": false,
    "all": true,
    "cache": false,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "html",
      "lcovonly",
      "text-summary"
    ],
    "exclude": [
      "**/*.d.ts",
      "frontend/**",
      "migrations/**",
      "coverage/**",
      "test/**",
      "dist/**",
      "**/node_modules/**"
    ]
  }
}
